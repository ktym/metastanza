{
  "@context": {
    "stanza": "http://togostanza.org/dist/resource/stanza#"
  },
  "@id": "http://togostanza.org/dist/metastanza/forcegraph",
  "stanza:label": "D3 forcegraph",
  "stanza:definition": "Visualize SPARQL results with d3sparql forcegraph.",
  "stanza:parameter": [
    {
      "stanza:key": "sparql_api",
      "stanza:example": "http://togostanza.org/sparqlist/api/d3sparql_forcegraph",
      "stanza:description": "SPARQL/REST API URL",
      "stanza:required": true
    },
    {
      "stanza:key": "title",
      "stanza:example": "D3 forcegraph",
      "stanza:description": "Title of visualization",
      "stanza:required": false
    },
    {
      "stanza:key": "width",
      "stanza:example": "1000",
      "stanza:description": "Width of canvas",
      "stanza:required": false
    },
    {
      "stanza:key": "height",
      "stanza:example": "500",
      "stanza:description": "Height of canvas",
      "stanza:required": false
    },
    {
      "stanza:key": "key1",
      "stanza:example": "lang1",
      "stanza:description": "Variable name for node1 (default is the first variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "key2",
      "stanza:example": "lang2",
      "stanza:description": "Variable name for node2 (default is the second variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "label1",
      "stanza:example": "lang1label",
      "stanza:description": "Variable name for the label of node1 (default is the third variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "label2",
      "stanza:example": "lang2label",
      "stanza:description": "Variable name for the label of node2 (default is the fourth variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "value1",
      "stanza:example": "lang1value",
      "stanza:description": "Variable name for the value of node1 (default is the fifth variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "value2",
      "stanza:example": "lang2value",
      "stanza:description": "Variable name for the value of node2 (default is the sixth variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "radius",
      "stanza:example": "12",
      "stanza:description": "Static value or a function to calculate radius of nodes",
      "stanza:required": false
    },
    {
      "stanza:key": "charge",
      "stanza:example": "-250",
      "stanza:description": "Force between nodes (repulsion if negative, attraction if positive)",
      "stanza:required": false
    },
    {
      "stanza:key": "distance",
      "stanza:example": "30",
      "stanza:description": "Target distance between linked nodes",
      "stanza:required": false
    },
    {
      "stanza:key": "label",
      "stanza:example": "label",
      "stanza:description": "Variable name for node labels",
      "stanza:required": false
    }
  ],
  "stanza:usage": "<togostanza-forcegraph sparql_api=\"http://togostanza.org/sparqlist/api/d3sparql_forcegraph\" title=\"D3 forcegraph\"></togostanza-forcegraph>",
  "stanza:type": "MetaStanza",
  "stanza:context": "Generic",
  "stanza:display": "Graph",
  "stanza:provider": "DBCLS",
  "stanza:license": "CC-BY",
  "stanza:author": "Toshiaki Katayama",
  "stanza:address": "ktym@dbcls.jp",
  "stanza:contributor": [
  ],
  "stanza:created": "2017-09-11",
  "stanza:updated": "2019-10-14"
}
