{
  "@context": {
    "stanza": "http://togostanza.org/dist/resource/stanza#"
  },
  "@id": "http://togostanza.org/dist/metastanza/dendrogram",
  "stanza:label": "D3 dendrogram",
  "stanza:definition": "Visualize SPARQL results with d3sparql dendrogram.",
  "stanza:parameter": [
    {
      "stanza:key": "sparql_api",
      "stanza:example": "http://togostanza.org/sparqlist/api/d3sparql_dendrogram?organism=Heterotardigrada",
      "stanza:description": "SPARQL/REST API URL",
      "stanza:required": true
    },
    {
      "stanza:key": "title",
      "stanza:example": "D3 dendrogram",
      "stanza:description": "Title of visualization",
      "stanza:required": false
    },
    {
      "stanza:key": "width",
      "stanza:example": "900",
      "stanza:description": "Width of canvas",
      "stanza:required": false
    },
    {
      "stanza:key": "height",
      "stanza:example": "4500",
      "stanza:description": "Height of canvas",
      "stanza:required": false
    },
    {
      "stanza:key": "root",
      "stanza:example": "root_name",
      "stanza:description": "Variable name for a root node (default is the first variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "parent",
      "stanza:example": "parent_name",
      "stanza:description": "Variable name of parent nodes (default is the second variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "child",
      "stanza:example": "child_name",
      "stanza:description": "Variable name of child nodes (default is the third variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "value",
      "stanza:example": "value_name",
      "stanza:description": "Variable name of numerical value of the child node (default is the fourth variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "margin",
      "stanza:example": "300",
      "stanza:description": "Margin width for labels",
      "stanza:required": false
    },
    {
      "stanza:key": "radius",
      "stanza:example": "5",
      "stanza:description": "Radius of node circles",
      "stanza:required": false
    }
  ],
  "stanza:usage": "<togostanza-dendrogram sparql_api=\"http://togostanza.org/sparqlist/api/d3sparql_dendrogram?organism=Heterotardigrada\" title=\"D3 dendrogram\"></togostanza-dendrogram>",
  "stanza:type": "MetaStanza",
  "stanza:context": "Generic",
  "stanza:display": "Tree",
  "stanza:provider": "DBCLS",
  "stanza:license": "CC-BY",
  "stanza:author": "Toshiaki Katayama",
  "stanza:address": "ktym@dbcls.jp",
  "stanza:contributor": [
  ],
  "stanza:created": "2017-09-11",
  "stanza:updated": "2018-04-06"
}
