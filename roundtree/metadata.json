{
  "@context": {
    "stanza": "http://togostanza.org/dist/resource/stanza#"
  },
  "@id": "http://togostanza.org/dist/metastanza/roundtree",
  "stanza:label": "D3 roundtree",
  "stanza:definition": "Visualize SPARQL results with d3sparql roundtree.",
  "stanza:parameter": [
    {
      "stanza:key": "sparql_api",
      "stanza:example": "http://togostanza.org/sparqlist/api/d3sparql_roundtree?organism=Ramazzottiidae",
      "stanza:description": "SPARQL/REST API URL",
      "stanza:required": true
    },
    {
      "stanza:key": "title",
      "stanza:example": "D3 roundtree",
      "stanza:description": "Title of visualization",
      "stanza:required": false
    },
    {
      "stanza:key": "diameter",
      "stanza:example": "800",
      "stanza:description": "Diameter of canvas",
      "stanza:required": false
    },
    {
      "stanza:key": "angle",
      "stanza:example": "360",
      "stanza:description": "Arc angle (less than 360 for wedge)",
      "stanza:required": false
    },
    {
      "stanza:key": "depth",
      "stanza:example": "200",
      "stanza:description": "Arc depth (less than diameter/2 - label length to fit)",
      "stanza:required": false
    },
    {
      "stanza:key": "radius",
      "stanza:example": "5",
      "stanza:description": "Radius of node circles",
      "stanza:required": false
    },
    {
      "stanza:key": "root",
      "stanza:example": "root_name",
      "stanza:description": "Variable name for a root node (default is the first variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "parent",
      "stanza:example": "parent_name",
      "stanza:description": "Variable name of parent nodes (default is the second variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "child",
      "stanza:example": "child_name",
      "stanza:description": "Variable name of child nodes (default is the third variable)",
      "stanza:required": false
    },
    {
      "stanza:key": "value",
      "stanza:example": "value_name",
      "stanza:description": "Variable name of numerical value of the child node (default is the fourth variable)",
      "stanza:required": false
    }
  ],
  "stanza:usage": "<togostanza-roundtree sparql_api=\"http://togostanza.org/sparqlist/api/d3sparql_roundtree?organism=Ramazzottius\" title=\"D3 roundtree\"></togostanza-roundtree>",
  "stanza:type": "MetaStanza",
  "stanza:context": "Generic",
  "stanza:display": "Tree",
  "stanza:provider": "DBCLS",
  "stanza:license": "CC-BY",
  "stanza:author": "Toshiaki Katayama",
  "stanza:address": "ktym@dbcls.jp",
  "stanza:contributor": [
  ],
  "stanza:created": "2017-09-04",
  "stanza:updated": "2019-10-14"
}
